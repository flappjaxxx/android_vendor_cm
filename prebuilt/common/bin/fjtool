192wakemax() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
echo '192000' > /system/etc/andrev_oc/wake_max
echo '192000' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
echo 'Tweaked' > /system/media/profile
rm -f /system/etc/init.d/84fj
echo 'echo '192000' > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq' > /system/etc/init.d/84fjwmax
chmod 755 /system/etc/init.d/84fjwmax
}

