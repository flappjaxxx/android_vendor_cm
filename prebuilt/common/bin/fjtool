svoice() {
FILE=svoice.tgz
MD5SUM=73dd516be224e8bfa0df7000c1f6ec92
URL="http://rootdev.org/i717/TU2/updates/svoice.tgz"
if [ ! -d /sdcard/download/tmp ]; then
  MK=1
  mkdir -p /sdcard/download/tmp
fi
cd /sdcard/download/tmp
if [ -f "$FILE" ]; then
MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then 
echo "Downloading Package Be Patient"
rm  "$FILE"
wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
echo "Mounting File System"
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd /sdcard/download/tmp
echo "Extracting Package"
  tar -xzf svoice.tgz
echo "Removing Old Files"
rm /data/app/com.vlingo.midas*.apk
echo "Copying Files"
  cp -R system /
echo "Setting Permissions"
chmod 644 /system/app/voicetalk.apk
echo "Cleaning up"
  cd - >/dev/null 2>&1
  echo "Done! Please Enjoy S-Voice"
else
  echo "Failed to Install S-Voice"
  echo "Try Again"
  exit 1
fi
rm  /sdcard/download/tmp/$FILE
rm -R /sdcard/download/tmp
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system

if [ "$MK" = "1" ]; then
  rm -R /sdcard/download/tmp   
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
fi
}

getswype() {
FILE=swype.tgz
MD5SUM=7afafec2abbf6b8bd9651d30e4cc2338
URL="http://rootdev.org/i717/cheeze/updates/swype.tgz"
if [ ! -d /sdcard/tmp ]; then
  MK=1
  mkdir -p /sdcard/tmp
fi
cd /sdcard/tmp
if [ -f "$FILE" ]; then
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then
echo "Downloading Package Be Patient"
  rm  "$FILE"
  wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
echo "Mounting File System"
  mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd /
cd /sdcard/tmp
echo "Extracting Package"
tar -xzf swype.tgz
echo "Installing Swype"
cp -R system /
echo "Setting Permissions"
chmod 644 /system/app/Swype.apk
  mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd - >/dev/null 2>&1
  echo "Done! Enjoy Swype!"
else
  echo "Failed to download / install Swype"
  exit 1
fi
rm  /sdcard/tmp/$FILE

if [ "$MK" = "1" ]; then
  rmdir /sdcard/tmp 
fi
}

ocgpu() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
rm /system/etc/init.d/99gpuoc
echo '#!/system/bin/sh
echo '300000000' > /sys/devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/max_gpuclk
echo '228571000' > /sys/devices/platform/kgsl-2d0.0/kgsl/kgsl-2d0/max_gpuclk
echo '228571000' > /sys/devices/platform/kgsl-2d1.1/kgsl/kgsl-2d1/max_gpuclk' > /system/etc/init.d/99gpuoc
chmod 777 /system/etc/init.d/99gpuoc
/system/etc/init.d/99gpuoc
echo "GPU Overclocked"
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
}

noocgpu() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
rm /system/etc/init.d/99gpuoc
echo '#!/system/bin/sh
echo '266667000' > /sys/devices/platform/kgsl-3d0.0/kgsl/kgsl-3d0/max_gpuclk
echo '200000000' > /sys/devices/platform/kgsl-2d0.0/kgsl/kgsl-2d0/max_gpuclk
echo '200000000' > /sys/devices/platform/kgsl-2d1.1/kgsl/kgsl-2d1/max_gpuclk' > /system/etc/init.d/99nogpuoc
chmod 777 /system/etc/init.d/99nogpuoc
/system/etc/init.d/99nogpuoc
rm /system/etc/init.d/99nogpuoc
echo "GPU clock speeds set back to normal"
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
}

