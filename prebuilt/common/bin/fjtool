noadblock() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
rm /system/etc/hosts
echo '127.0.0.1		    localhost' > /system/etc/hosts
chmod 644 /system/etc/hosts
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
echo Restored stock hosts file
echo Ad Blocking Disabled
echo Reboot required!
}

