themesdisabled() {
FILE=themesupport.tgz
MD5SUM=9d4758282e721d5ad2446b6806eaee28
URL="http://rootdev.org/i717/TU2/updates/themesupport.tgz"
if [ ! -d /sdcard/tmp ]; then
  MK=1
  mkdir -p /sdcard/tmp
fi
cd /sdcard/tmp
if [ -f "$FILE" ]; then
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" != "1" ]; then
echo "Downloading Package Be Patient"
  rm  "$FILE"
  wget "$URL"
  MD5=`md5sum $FILE | awk '{ print $1 }'`
  if [ "$MD5" = "$MD5SUM" ]; then
     GOOD=1
  fi
fi
if [ "$GOOD" = "1" ]; then
echo "Mounting File System"
  mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd /
cd /sdcard/tmp
echo "Extracting Package"
tar -xzf themesupport.tgz
echo "Installing Theme Support"
cp -R system /
echo "Setting Permissions"
chmod 644 /system/app/ThemeChooser.apk
chmod 644 /system/app/ThemeManager.apk
chmod 644 /system/etc/permissions/com.tmobile.software.themes.xml
  mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
  cd - >/dev/null 2>&1
  echo "Done! Reboot and enjoy silly theme support!"
  echo "Theme Chooser will now be in your App Drawer NOT in Settings"
else
  echo "Failed to download / install Theme Support"
  exit 1
fi
rm  /sdcard/tmp/$FILE

if [ "$MK" = "1" ]; then
  rmdir /sdcard/tmp 
fi
}

fastcharge() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
rm /system/etc/init.d/98fastcharge
echo '#!/system/bin/sh
echo '1' > /sys/kernel/fast_charge/force_fast_charge' > /system/etc/init.d/98fastcharge
chmod 777 /system/etc/init.d/98fastcharge
/system/etc/init.d/98fastcharge
echo "Fast Charge Enabled"
eco "* /sys/kernel/fast_charge/force_fast_charge"
echo "*   1 - substitute AC to USB charging always"
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
}

fastcharge2() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
rm /system/etc/init.d/98fastcharge
echo '#!/system/bin/sh
echo '2' > /sys/kernel/fast_charge/force_fast_charge' > /system/etc/init.d/98fastcharge
chmod 777 /system/etc/init.d/98fastcharge
/system/etc/init.d/98fastcharge
echo "Fast Charge Option 2 Enabled"
echo "* /sys/kernel/fast_charge/force_fast_charge"
echo "*   2 - substitute AC to USB charging only if no USB peripheral is detected"
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
}

nofastcharge() {
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
rm /system/etc/init.d/98fastcharge
echo '#!/system/bin/sh
echo '0' > /sys/kernel/fast_charge/force_fast_charge' > /system/etc/init.d/98nofastcharge
chmod 777 /system/etc/init.d/98nofastcharge
/system/etc/init.d/98nofastcharge
rm /system/etc/init.d/98nofastcharge
echo "Fast Charge Disabled"
echo ":* /sys/kernel/fast_charge/force_fast_charge"
echo "*   0 - disabled -default-"
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
}

