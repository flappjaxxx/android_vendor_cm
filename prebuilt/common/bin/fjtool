chmod 644 /system/etc/hosts
rm /sdcard/tmp/hosts.txt
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
echo Installed Custom hosts file
echo Ad Blocking Enabled
echo Reboot required to apply changes!
}

upfjtools() {
rm -R /sdcard/fjtoolstmp
mkdir /sdcard/fjtoolstmp
cd /sdcard/fjtoolstmp
echo "Downloading Package Be Patient"
wget http://rootdev.org/i717/TU2/updates/fjtoolsupdateics16.tgz
echo "Extracting Package"
tar -xzf fjtoolsupdateics16.tgz
echo "Mounting File Systems"
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p24 /system
mount -o rw,remount -t yaffs2 /dev/block/mmcblk0p25 /data
cd /sdcard/fjtoolstmp/system
echo "Copying New Files"
cp -R bin /system
cp -R media /system
echo "Setting Permissions"
chmod 755 /system/bin/fjtool
cd app
echo "Updating Tools App"
pm install -r FJTools-Note.apk
echo "Removing Old Files"
rm /system/app/FJTools-Note.apk
sleep 3
rm -R /sdcard/fjtoolstmp
echo "Done! FJTools Updated"
mount -o ro,remount -t yaffs2 /dev/block/mmcblk0p24 /system
}

